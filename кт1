class TreeNode {    
  int data;
    
  TreeNode left;
    
    TreeNode right;
    
    public TreeNode(int value) {
        data = value;    
        left = null;     
        right = null;    
    }
}

public class Main {
    public static void main(String[] args) {
        TreeNode root = new TreeNode(10);
        
        root.left = new TreeNode(5);
        
        root.right = new TreeNode(15);
        
        root.left.left = new TreeNode(3);
        root.left.right = new TreeNode(7);
    }
}
